{
	"name": "vs-nes",
	"displayName": "vs-nes",
	"description": "nes emulator for vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vs-nes.nesRun"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vs-nes.nesRun",
				"title": "Nes Run"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"build:nes": "cross-env NODE_ENV=production webpack",
		"dev:nes": "cross-env NODE_ENV=development webpack-dev-server"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"html-webpack-plugin": "^4.3.0",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"nes-js": "0.0.1"
	}
}
